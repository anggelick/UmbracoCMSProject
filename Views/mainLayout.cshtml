@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
	var pageTitle = !string.IsNullOrEmpty(ViewBag.Title) ? $"{ViewBag.Title} | Onatrix - Financial Solutions for Your Business" : "Onatrix - Financial Solutions for Your Business";
}


<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="icon" type="image/x-icon" href="/images/favicon.ico">
	<script src="https://kit.fontawesome.com/0da086c9c3.js" crossorigin="anonymous"></script>
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
	<link rel="stylesheet" href="/css/scss/main.css">
	<title>@pageTitle</title>
</head>
<body>
	<div class="wrapper">
		@await Html.PartialAsync("partials/static/TopBar.cshtml")

		@await Html.PartialAsync("partials/static/Header.cshtml")

		@if (Model.ContentType.Alias != "homePage")
        {
            @await Html.PartialAsync("partials/static/breadcrumb.cshtml")
        }
		

		<main>
			@RenderBody()
		</main>


		@await Html.PartialAsync("partials/static/Footer.cshtml")

	</div>


	<script src="/scripts/main.js"></script>
</body>
</html>